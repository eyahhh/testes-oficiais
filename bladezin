      Enabled = true,
      Invite = "8S2bNRYd",
      RememberJoins = true
   },
   KeySystem = false,
   KeySettings = {
      Title = "please get your key!",
      Subtitle = "Key System",
      Note = "Get Key in https://fir3.net/AB3XV9KLrg",
      FileName = "Key",
      SaveKey = true,
