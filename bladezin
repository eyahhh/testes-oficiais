Rayfield:LoadConfiguration()
getgenv().SecureMode = true

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Lightz Hub",
   LoadingTitle = "Lightz Hub",
   LoadingSubtitle = "by Light",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Lightz Hub"
   },
