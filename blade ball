local function AddButton(tab, name, callback)
    tab:AddButton({
        Name = name,
        Callback = function()
            print("button pressed")
            loadstring(game:HttpGet(callback))()
        end
    })
end

AddButton(ParryTab, "Auto Parry", "https://raw.githubusercontent.com/SadlekAski/Scripts/main/Blade%20Ball/Ping-Based%20Parry.lua")
AddButton(ParryTab, "Op Auto Spam", "https://pastebin.com/raw/t2391h1A")
