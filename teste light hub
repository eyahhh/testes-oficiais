local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Testing it", HidePremium = false, SaveConfig = true, ConfigFolder = "SaveLight"})
local Tabe = Window:MakeTab({
    Name = "LIGHTZ",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
Tabe:AddButton({
    Name = "autoclick",
    Callback = function(switch)
        print("button pressed")
        -- Verifique se 'value' está definido e tem um valor atribuído
        if switch == true then 
        print("você ativou o autoclick")
        end
    end
})
OrionLib:Init()
