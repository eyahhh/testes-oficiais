local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Lightz Hub",
   LoadingTitle = "Lightz Hub",
   LoadingSubtitle = "by Light",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Lightz Hub"
   },
   Discord = {
      Enabled = true,
      Invite = "xRBvtwHB",
      RememberJoins = true
   },
   KeySystem = false,
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})

local Tab = Window:CreateTab("Combat", 4483362458)

wait(2.6)

Rayfield:Notify({
   Title = "Lightz Hub",
   Content = "Script Carregado!",
   Duration = 6.5,
   Image = 4483362458,
   Actions = {
      Ignore = {
         Name = "Ok!",
         Callback = function()
            print("The user tapped Ok!")
         end
      },
   },
})


local Button2 = Tab:CreateButton({
   Name = "Auto Parry Op",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/eyahhh/testes-oficiais/main/AUTO%20PARRY%20V2"))()
   end,
   Discord = {
      Enabled = true,
      Invite = "xRBvtwHB", -- Discord invite code for the button, if different from the main window Discord invite
      RememberJoins = true -- Whether or not to remember joins for the button's Discord invite
   }
})


local Button2 = Tab:CreateButton({
   Name = "Auto Spam Op",
